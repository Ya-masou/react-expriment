{
  "name": "react-practice",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --ignorePath .gitignore --watch -o './src/lib'",
    "build": "pathpida --ignorePath .gitignore && next build",
    "start": "next start",
    "lint": "eslint './**/*.{js,ts,tsx}' --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix",
    "format": "yarn prettier --write './**/*.{js,ts,tsx}'"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.1",
    "@chakra-ui/react": "^1.7.4",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "framer-motion": "^5",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "17.0.8",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "8.6.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.17.0",
    "prettier": "^2.5.1",
    "typescript": "4.5.4"
  }
}
